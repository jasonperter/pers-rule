[General]
# 跳过代理使用
skip-proxy = %APPEND% www.localnetwork.up,193.168.0.1，www.baidu.com,yunbusiness.ccb.com,wxh.wo.cn,gate.lagou.com,www.abchina.com.cn,www.shanbay.com,login-service.mobile-bank.psbc.com,mobile-bank.psbc.com,id6.me,www.163.com
always-real-ip = %APPEND% www.localnetwork.up,easy-login.10099.com.cn,*-update.xoyocdn.com,id6.me,open.e.189.cn

[Script]
GitHub = type=http-request, script-path=Script/header.js, pattern=^https:\/\/(?:raw|gist|avatars)\.githubusercontent\.com, max-size=0, timeout=30, script-update-interval=0, enable=true

[MITM]
hostname = %APPEND% raw.githubusercontent.com, gist.githubusercontent.com, avatars.githubusercontent.com

